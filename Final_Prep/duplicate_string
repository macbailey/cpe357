#include <stdio.h>
#include <stdlib.h>

char *duplicate_string(char *s)
{
  int i = 0; 
  char *ret_s = malloc(sizeof(s));
  while(s[i] != '\0')
  {
    ret_s[i] = s[i];
    i++;
  }
  ret_s[i] = '\0';
  return ret_s;
}

int main() 
{ 
  char s[] = "Here is a string"; 
  printf("%s \n", duplicate_string(s));
  return 0; 
}